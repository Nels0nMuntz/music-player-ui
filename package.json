{
  "name": "music-player-ui",
  "description": "A music player UI component library built with React and Tailwind CSS",
  "keywords": [
    "react",
    "tailwindcss",
    "ui-components",
    "music-player"
  ],
  "homepage": "https://github.com/Nels0nMuntz/music-player-ui",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Nels0nMuntz/music-player-ui"
  },
  "private": false,
  "author": {
    "name": "Eugene Chekerdes",
    "email": "eugene.checkerdes@gmail.com"
  },
  "version": "0.0.1",
  "type": "module",
  "main": "dist/music-player-ui.es.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/music-player-ui.es.js",
      "require": "./dist/music-player-ui.umd.js"
    },
    "./styles.css": {
      "require": "./dist/styles.css",
      "default": "./dist/styles.css"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build && npm run build:css",
    "build:css": "tailwindcss -m -i ./src/styles.css -o ./dist/styles.css",
    "lint": "eslint .",
    "preview": "vite preview",
    "sb": "concurrently \"npm run sb:css\" \"storybook dev -p 6006\"",
    "sb:css": "tailwindcss -w -i ./src/styles.css -o ./src/index.css",
    "build-sb": "concurrently \"npm run build-sb:css\" \"storybook build\"",
    "build-sb:css": "tailwindcss -m -i ./src/styles.css -o ./src/index.css",
    "prepublishOnly": "npm run build"
  },
  "peerDependencies": {
    "@radix-ui/react-dialog": "^1.1.10",
    "@radix-ui/react-slot": "^1.2.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "lucide-react": "^0.501.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "tailwind-merge": "^3.2.0",
    "tw-animate-css": "^1.2.5"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^4.0.1",
    "@eslint/js": "^9.30.1",
    "@storybook/addon-a11y": "^9.0.16",
    "@storybook/addon-docs": "^9.0.16",
    "@storybook/addon-vitest": "^9.0.16",
    "@storybook/react-vite": "^9.0.16",
    "@tailwindcss/cli": "^4.1.11",
    "@tailwindcss/vite": "^4.1.11",
    "@types/node": "^24.0.13",
    "@types/react": "^18.3.1",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react": "^4.6.0",
    "@vitest/browser": "^3.2.4",
    "@vitest/coverage-v8": "^3.2.4",
    "concurrently": "^9.2.0",
    "eslint": "^9.30.1",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "eslint-plugin-storybook": "^9.0.16",
    "globals": "^16.3.0",
    "playwright": "^1.54.0",
    "storybook": "^9.0.16",
    "tailwindcss": "^4.1.11",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.35.1",
    "vite": "^7.0.4",
    "vite-plugin-dts": "^4.5.4",
    "vitest": "^3.2.4"
  }
}
